{
  "name": "readflex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --config .eslintrc.json",
    "type-check": "tsc --noEmit",
    "test:unit": "jest --bail --passWithNoTests",
    "test:watch": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "fix:all": "npm run format && npm run lint -- --fix && npm run type-check && npm run test:unit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"__tests__/**/*.{ts,tsx}\"",
    "precommit": "npm run fix:all",
    "prepush": "npm run test:unit && npm run build"
  },
  "dependencies": {
    "@axe-core/react": "^4.11.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^5.6.0",
    "@tanstack/react-query": "^5.90.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^10.16.0",
    "next": "14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-focus-lock": "^2.13.6",
    "react-hook-form": "^7.66.0",
    "react-hot-toast": "^2.6.0",
    "use-debounce": "^10.0.6",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.5.0",
    "@storybook/addon-essentials": "^7.5.0",
    "@storybook/addon-interactions": "^7.5.0",
    "@storybook/nextjs": "^7.5.0",
    "@storybook/react": "^7.5.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "20.19.25",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "14.0.0",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.0",
    "prettier": "^3.6.2",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.0",
    "typescript": "5.9.3"
  }
}
